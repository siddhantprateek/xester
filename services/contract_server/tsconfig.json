{
  "compilerOptions": {
      "target": "es6",
      "allowJs": true,
      "skipLibCheck": true,
      "module": "commonjs",
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "noEmit": true,
      "resolveJsonModule": true,
    },
    "exclude": [
        "node_modules"
    ]
}